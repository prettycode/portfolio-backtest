{
    "name": "portfolio-backtest",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "author": {
        "name": "Chris O'Brien",
        "url": "https://github.com/prettycode"
    },
    "license": "cc-by-nc-4.0",
    "scripts": {
        "start": "npm run install && npm run build:ci  && vite --open",
        "versions": "echo node: && node -v && echo npm: && npm -v",
        "install:ci": "npm ci",
        "install:local": "npm install",
        "outdated:ci": "npm outdated",
        "outdated:fix": "npm update --save",
        "prettier:ci": "npx prettier . --check",
        "prettier:fix": "npx prettier . --write",
        "lint:ci": "eslint . --max-warnings 0",
        "lint:fix": "npx eslint . --fix",
        "audit:fix": "npm audit fix --force",
        "audit:ci": "npm audit --audit-level=none",
        "vite:build:local": "tsc & vite build --mode development",
        "vite:build:ci": "tsc && vite build",
        "build:local": "npm run versions && npm run install:local && npm run outdated:fix && npm run audit:fix && npm run prettier:fix && npm run lint:fix && npm run vite:build:local && npm run test",
        "build:ci": "npm run versions && npm run install:ci && npm run outdated:ci && npm run audit:ci && npm run prettier:ci && npm run lint:ci && npm run vite:build:ci && npm run test",
        "test:local": "npm run vite:build:local && npx jest",
        "test": "npx jest",
        "precommit": "npm run build:local && npm run build:ci && vite --open"
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^6.4.2",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "bootstrap": "^5.3.1",
        "font-awesome": "^4.7.0",
        "lodash.clonedeep": "^4.5.0",
        "lodash.groupby": "^4.6.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.15.0",
        "react-select": "^5.7.4",
        "reactstrap": "^9.2.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.4",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/lodash.groupby": "^4.6.7",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@types/react-fontawesome": "^1.6.5",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "eslint": "^8.48.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "jest": "^29.6.4",
        "prettier": "^3.0.2",
        "ts-jest": "^29.1.1",
        "typescript": "^5.2.2",
        "vite": "^4.4.9"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node"
    }
}
