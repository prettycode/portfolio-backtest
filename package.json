{
    "name": "portfolio-backtest",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "npm install && npm run prettier:fix && npm run lint:fix && npm run build && npm run test && vite --open",
        "prettier:ci": "npx prettier . --check",
        "prettier:fix": "npx prettier . --write",
        "lint:ci": "eslint . --ext ts,tsx --max-warnings 0",
        "lint:fix": "npx eslint . --ext .ts,.tsx --fix",
        "build": "tsc && vite build",
        "build:ci": "npm ci && npm run prettier:ci && npm run lint:ci && npm run build && npm run test",
        "test": "npx jest"
    },
    "dependencies": {
        "bootstrap": "^5.3.1",
        "lodash.clonedeep": "^4.5.0",
        "lodash.groupby": "^4.6.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.3",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/lodash.groupby": "^4.6.7",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "jest": "^29.6.2",
        "prettier": "^3.0.1",
        "ts-jest": "^29.1.1",
        "typescript": "^5.0.2",
        "vite": "^4.4.5"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node"
    }
}
